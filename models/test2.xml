1 <?xml version="1.0" encoding="UTF-8"?>
2 <sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
3 <model>
4 <listOfCompartments>
5 <compartment name="cell" volume="1"/>
6 </listOfCompartments>
7 <listOfSpecies>
8 <specie name="s1" compartment="cell" initialAmount="4"/>
9 <specie name="s2" compartment="cell" initialAmount="2"/>
10 <specie name="x0" compartment="cell" initialAmount="1"/>
11 <specie name="x1" compartment="cell" initialAmount="0"/>
12 </listOfSpecies>
13 <listOfParameters>
14 <parameter name="k1" value="1.2"/>
15 <parameter name="k2" value="1000"/>
16 <parameter name="k3" value="3000"/>
17 <parameter name="k4" value="4.5"/>
18 </listOfParameters>
19 <listOfRules>
20 <parameterRule name="t" formula="s1 + s2"/>
21 <parameterRule name="k" formula="k3/k2"/>
22 <specieConcentrationRule specie="s2" formula="k * t/(1 + k)"/>
23 <specieConcentrationRule specie="s1" formula="t - s2"/>
24 </listOfRules>
25 <listOfReactions>
26 <reaction name="j1">
27 <listOfReactants>
28 <specieReference specie="x0"/>
29 </listOfReactants>
30 <listOfProducts>
31 <specieReference specie="s1"/>
32 </listOfProducts>
33 <kineticLaw formula="k1 * x0"/>
34 </reaction>
35 <reaction name="j3">
36 <listOfReactants>
37 <specieReference specie="s2"/>
38 </listOfReactants>
39 <listOfProducts>
40 <specieReference specie="x1"/>
41 </listOfProducts>
42 <kineticLaw formula="k4 * s2"/>
43 </reaction>
44 </listOfReactions>
45 </model>
46 </sbml>